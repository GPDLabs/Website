{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "passThroughEnv": ["PORT"]
    },
    "type-check": {
      "cache": false
    }
  }
}
